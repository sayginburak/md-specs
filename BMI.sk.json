    "intro": {
      "headerTitle": "vysvetlenie",
      "infoItems": {
        "instructions1": {
          "type": "text",
          "title": "Index telesnej hmotnosti (BMI) je jednoduchý index hmotnosti na výšku, ktorý sa bežne používa na klasifikáciu podváhy, nadváhy a obezity u dospelých. Je definovaná ako hmotnosť v kilogramoch vydelená druhou mocninou výšky v metroch (kg / m²).",
          "resultHasRichText": true,
          "titleHasRichText": true,
          "explanationHasRichText": true
        }
      },
      "resultHasRichText": true,
      "titleHasRichText": true,
      "explanationHasRichText": true
    },
    "formula": {
      "headerTitle": "vzorec",
      "infoItems": {
        "formula1": {
          "type": "text",
          "title": "BMI = hmotnosť / výška²",
          "resultHasRichText": true,
          "titleHasRichText": true,
          "explanationHasRichText": true
        }
