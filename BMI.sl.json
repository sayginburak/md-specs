    "intro": {
      "headerTitle": "Pojasnilo",
      "infoItems": {
        "instructions1": {
          "type": "besedilo",
          "title": "Indeks telesne mase (BMI) je preprost indeks teže za višino, ki se običajno uporablja za razvrščanje prenizke teže, prekomerne teže in debelosti pri odraslih. Opredeljena je kot teža v kilogramih, deljena s kvadratom višine v metrih (kg / m²).",
          "resultHasRichText": true,
          "titleHasRichText": true,
          "explanationHasRichText": true
        }
      },
      "resultHasRichText": true,
      "titleHasRichText": true,
      "explanationHasRichText": true
    },
    "formula": {
      "headerTitle": "Formula",
      "infoItems": {
        "formula1": {
          "type": "besedilo",
          "title": "BMI = teža / višina²",
          "resultHasRichText": true,
          "titleHasRichText": true,
