      "infoItems": {
        "instructionsForUse1": {
          "type": "besedilo",
          "title": "<b> Splošni opis </b> <br> Indeks telesne mase (BMI) je najpogosteje uporabljena antropometrična metoda za merjenje debelosti, ki se izračuna kot teža posameznika v kilogramih, deljena z višino v kvadratnih metrih.",
          "titleHasRichText": true
        },
        "instructionsForUse2": {
          "type": "besedilo",
          "title": "<b> Predvideni uporabniki </b> <br> zdravstveni delavci",
          "titleHasRichText": true
        },
        "instructionsForUse3": {
          "type": "besedilo",
          "title": "<b> Predvidena uporaba </b> <br> Izračun indeksa telesne mase (ITM)",
          "titleHasRichText": true
        },
        "instructionsForUse4": {
          "type": "besedilo",
          "title": "<b> Namenska populacija bolnikov </b> <br> Odrasli",
